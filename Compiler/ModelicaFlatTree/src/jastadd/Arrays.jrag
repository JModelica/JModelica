    public FArrayExpSubscripts FArrayLitSubscripts.copyAsFArrayExpSubscripts() {
        FSubscript[] list = new FSubscript[subscripts.length];
        for (int i = 0; i < list.length; i++) {
            list[i] = new FIntegerSubscript(subscripts[i]);
        }
        return new FArrayExpSubscripts(new List(list));
    }

