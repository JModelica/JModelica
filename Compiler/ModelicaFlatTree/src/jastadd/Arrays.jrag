    syn lazy ArrayExp FAbstractArrayExp.getArrayExp() {
        if (useTempVar())
            return createTempArray();
        
        ArrayExp res = new ArrayExp(indices());
        for (Index i : res.indices())
            res.set(i, new FSubscriptedExp(unboundCopy(), i.createFArraySubscripts()));
        return res;
