		Index ti = new Index(ind);
		for (Index i : res.indices()) {
			ind[0] = i.get(0);
			ind[1] = i.get(1);
			if (ind[0] > ind[1]) {
				int temp = ind[0];
				ind[0] = ind[1];
				ind[1] = temp;
			}
			res.set(i, inner.get(ti));
		}
		return res;
