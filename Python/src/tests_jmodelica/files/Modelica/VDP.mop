package VDP_pack
    
   model VDP
    // State start values
    parameter Real x1_0 = 0;
    parameter Real x2_0 = 1;

    // The states
    Real x1(start = x1_0);
    Real x2(start = x2_0);

    // The control signal
    input Real u;

  equation
    der(x1) = (1 - x2^2) * x1 - x2 + u;
    der(x2) = x1;
  end VDP;
    
    
  optimization VDP_Opt (objective = cost(finalTime),
                         startTime = 0,
                         finalTime = 20)

    parameter Real p1 = 1;             // Parameter 1 
    parameter Real p2 = 1;             // Parameter 2 
    parameter Real p3 = 2;             // Parameter 3 

    // The states
    Real x1(start=0,fixed=true);
    Real x2(start=1,fixed=true);

    // The control signal
    input Real u;

    Real cost(start=0,fixed=true);

  equation
    der(x1) = (1 - x2^2) * x1 - x2 + u;
    der(x2) = p1*x1;
    der(cost) = exp(p3) * (x1^2 + x2^2 + u^2);
  constraint 
     u<=0.75;
  end VDP_Opt;
        
    optimization VDP_Opt_Simple (objectiveIntegrand = x1^2 + x2^2 + u^2,
                      startTime = 0,
                      finalTime = 20)
      // The states
      Real x1(start=0,fixed=true);
      Real x2(start=1,fixed=true);
      // The control signal
      input Real u;
    equation
      der(x1) = (1 - x2^2) * x1 - x2 + u;
      der(x2) = x1;
    constraint 
       u<=0.75;
    end VDP_Opt_Simple;
  
  optimization VDP_Opt2 (objective = cost(finalTime),
                         startTime = 0,
                         finalTime = 20)

    parameter Real p1 = 1;             // Parameter 1 
    parameter Real p2 = 1;             // Parameter 2 
    parameter Real p3 = 2;             // Parameter 3 

    // The states
    Real x1(start=0,fixed=true);
    Real x2(start=1,fixed=true);

    // The control signal
    input Real u(max=0.75);

    Real cost(start=0,fixed=true,min=0);

  equation
    der(x1) = (1 - x2^2) * x1 - x2 + u;
    der(x2) = p1*x1;
    der(cost) = exp(p3) * (x1^2 + x2^2 + u^2);
  end VDP_Opt2;
  
